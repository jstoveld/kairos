{
  "Version": "2012-10-17",
  "Statement": [
      {
          "Effect": "Allow",
          "Action": [
              "sqs:ReceiveMessage",
              "sqs:DeleteMessage",
              "sqs:GetQueueAttributes"
          ],
          "Resource": "arn:aws:sqs:us-east-1:*:kairos-image-processing-queue-*"
      },
      {
          "Effect": "Allow",
          "Action": "s3:ListBucket",
          "Resource": [
              "arn:aws:s3:::kairos-app-bucket-prod",
              "arn:aws:s3:::kairos-app-bucket-nonprod"
          ]
      },
      {
          "Effect": "Allow",
          "Action": [
              "s3:GetObject",
              "s3:PutObject"
          ],
          "Resource": [
              "arn:aws:s3:::kairos-app-bucket-prod/*",
              "arn:aws:s3:::kairos-app-bucket-nonprod/*"
          ]
      },
      {
          "Effect": "Allow",
          "Action": [
              "sns:Publish",
              "sns:Subscribe",
              "sns:Unsubscribe"
          ],
          "Resource": "arn:aws:sns:us-east-1:060006743195:kairos-transformation-complete-*"
      },
      {
          "Effect": "Allow",
          "Action": [
              "logs:CreateLogGroup",
              "logs:CreateLogStream",
              "logs:PutLogEvents"
          ],
          "Resource": "arn:aws:logs:*:*:*"
      }
  ]
}