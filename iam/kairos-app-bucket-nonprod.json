{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "AllowKairosS3RoleAccess",
            "Effect": "Allow",
            "Principal": {
                "AWS": "arn:aws:iam::060006743195:role/kairos_s3_role"
            },
            "Action": [
                "s3:ListBucket",
                "s3:GetObject",
                "s3:PutObject",
                "s3:DeleteObject"
            ],
            "Resource": [
                "arn:aws:s3:::kairos-app-bucket-nonprod",
                "arn:aws:s3:::kairos-app-bucket-nonprod/*"
            ]
        }
    ]
}
